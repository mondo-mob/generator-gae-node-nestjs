{
  "name": "<%= slugify(project) %>-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/moment": "1.0.2",
    "@material-ui/core": "4.2.0",
    "@material-ui/icons": "4.2.1",
    "@material-ui/pickers": "3.1.2",
    "apollo-cache": "1.3.2",
    "apollo-cache-inmemory": "1.6.2",
    "apollo-client": "2.6.3",
    "apollo-link": "1.2.12",
    "apollo-link-batch-http": "1.2.12",
    "apollo-link-error": "1.1.11",
    "classnames": "2.2.6",
    "final-form": "4.11.0",
    "graphql": "14.4.2",
    "graphql-tag": "2.10.1",
    "http-proxy-middleware": "^0.19.1",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "prop-types": "15.6.2",
    "query-string": "6.2.0",
    "react": "16.8.2",
    "react-apollo": "2.5.8",
    "react-apollo-hooks": "0.5.0",
    "react-dom": "16.8.2",
    "react-dropzone": "7.0.1",
    "react-final-form": "4.0.2",
    "react-progress-bar-plus": "1.3.1",
    "react-router-dom": "5.0.1",
    "tslint-react": "^3.6.0",
    "universal-cookie": "3.0.7"
  },
  "scripts": {
    "format": "prettier --write \"./src/**/*.ts{,?}\"",
    "lint": "tslint --project .",
    "start": "npm run lint && react-scripts start",
    "build": "npm run lint && react-scripts build && mv ./build/ ../server/public",
    "eject": "react-scripts eject",
    "introspect": "apollo service:download --endpoint=http://localhost:8080/api/graphql --header='x-csrf-token:development'",
    "generate": "apollo client:codegen --includes=\"src/**/*.ts{,?}\" --tagName=gql --localSchemaFile=schema.json --target=typescript --outputFlat src/graphql.ts",
    "graphql": "npm run introspect && npm run generate",
    "kill": "bash ./scripts/kill.sh",
    "kill-force": "bash ./scripts/kill.sh -9"
  },
  "devDependencies": {
    "@types/classnames": "2.2.7",
    "@types/graphql": "14.2.2",
    "@types/jest": "23.3.13",
    "@types/lodash": "4.14.120",
    "@types/node": "10.12.20",
    "@types/prop-types": "15.5.8",
    "@types/query-string": "6.2.0",
    "@types/react": "16.8.2",
    "@types/react-dom": "16.8.0",
    "@types/react-dropzone": "4.2.2",
    "@types/react-router": "4.4.3",
    "@types/react-router-dom": "4.3.1",
    "apollo": "2.15.0",
    "prettier": "1.18.2",
    "react-scripts": "^2.1.5",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react-hooks": "2.1.1",
    "typescript": "3.5.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
