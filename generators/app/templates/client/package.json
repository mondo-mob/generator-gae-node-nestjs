{
  "name": "test-nest-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/moment": "1.0.2",
    "@material-ui/core": "3.9.1",
    "@material-ui/icons": "3.0.2",
    "apollo-cache": "1.1.25",
    "apollo-cache-inmemory": "1.4.2",
    "apollo-client": "2.4.12",
    "apollo-link": "1.2.6",
    "apollo-link-batch-http": "1.2.6",
    "apollo-link-error": "1.1.7",
    "classnames": "2.2.6",
    "final-form": "4.11.0",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.1",
    "http-proxy-middleware": "^0.19.1",
    "lodash": "4.17.11",
    "material-ui-pickers": "2.1.2",
    "moment": "2.24.0",
    "prop-types": "15.6.2",
    "query-string": "6.2.0",
    "react": "16.8.2",
    "react-apollo": "2.4.1",
    "react-apollo-hooks": "0.4.5",
    "react-dom": "16.8.2",
    "react-dropzone": "7.0.1",
    "react-final-form": "4.0.2",
    "react-progress-bar-plus": "1.3.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "tslint-react": "^3.6.0",
    "universal-cookie": "3.0.7"
  },
  "scripts": {
    "format": "prettier --write \"./src/**/*.ts{,?}\"",
    "lint": "tslint --project .",
    "start": "npm run lint && react-scripts start",
    "build": "npm run lint && react-scripts build && mv ./build/ ../server/public",
    "eject": "react-scripts eject",
    "introspect": "apollo-codegen introspect-schema http://localhost:8080/api/graphql --output graphql.schema.json --header='x-csrf-token:development'",
    "generate": "apollo-codegen generate \"src/**/*.ts{,?}\" --tag-name gql --schema graphql.schema.json --target typescript --output src/graphql.ts",
    "graphql": "echo 'TEMPORARY WORKAROUND FOR APOLLO-CODEGEN / GRAPHQL 14 INCOMPATIBILITY' && rm -rf node_modules/apollo-codegen/node_modules && npm run introspect && npm run generate",
    "kill": "bash ./scripts/kill.sh",
    "kill-force": "bash ./scripts/kill.sh -9"
  },
  "devDependencies": {
    "@types/classnames": "2.2.7",
    "@types/graphql": "14.0.5",
    "@types/jest": "23.3.13",
    "@types/lodash": "4.14.120",
    "@types/node": "10.12.20",
    "@types/prop-types": "15.5.8",
    "@types/query-string": "6.2.0",
    "@types/react": "16.8.2",
    "@types/react-dom": "16.8.0",
    "@types/react-dropzone": "4.2.2",
    "@types/react-router": "4.4.3",
    "@types/react-router-dom": "4.3.1",
    "apollo-codegen": "0.20.2",
    "prettier": "1.18.2",
    "react-scripts": "^2.1.5",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
