{
  "name": "<%= slugify(project) %>-server",
  "version": "1.0.0",
  "description": "<%= slugify(projectName) %> built with node + typescript + react",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start:dev": "CLOUDSDK_CORE_PROJECT=<%= slugify(project) %>-development npx server",
    "format": "prettier --write \"./src/**/*.ts\" && prettier --write \"./src/**/*.graphqls\"",
    "start": "node ./dist/main.js",
    "prestart:dev": "rm -rf dist/",
    "start:debug": "ts-node --inspect src/main.ts",
    "compile:prod": "rm -rf dist && tsc",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build": "rm -rf public && npm run --prefix ../client build && npm run compile:prod",
    "deploy:dev": "CLOUDSDK_CORE_PROJECT=<%= slugify(project) %>-dev npm run deploy && TAG_NAME=dev/`date +%Y%m%d_%H%M%S` && git tag -a $TAG_NAME -m 'dev deployed' && git push origin $TAG_NAME",
    "deploy:uat": "CLOUDSDK_CORE_PROJECT=<%= slugify(project) %>-uat npm run deploy && TAG_NAME=uat/`date +%Y%m%d_%H%M%S` && git tag -a $TAG_NAME -m 'uat deployed' && git push origin $TAG_NAME",
    "deploy:prod": "CLOUDSDK_CORE_PROJECT=<%= slugify(project) %>-prod npm run deploy && TAG_NAME=prod/`date +%Y%m%d_%H%M%S` && git tag -a $TAG_NAME -m 'prod deployed' && git push origin $TAG_NAME",
    "deploy": "npm run build && gcloud app deploy",
    "dsui": "dsui --port 4000",
    "kill-server": "bash ./scripts/kill-server.sh",
    "kill-server-force": "bash ./scripts/kill-server.sh -9"
  },
  "dependencies": {
    "@3wks/gae-node-nestjs": "^0.1.9",
    "@google-cloud/connect-datastore": "^1.0.0",
    "@google-cloud/datastore": "^1.4.1",
    "@google-cloud/debug-agent": "^2.6.0",
    "@google-cloud/logging-bunyan": "^0.8.2",
    "@google-cloud/storage": "^1.7.0",
    "@google-cloud/trace-agent": "^2.10.1",
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/testing": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.12",
    "colors": "^1.3.0",
    "config": "^1.30.0",
    "csurf": "^1.9.0",
    "csvtojson": "^2.0.7",
    "dataloader": "^1.4.0",
    "ejs": "^2.6.1",
    "email-addresses": "^3.0.1",
    "faker": "^4.1.0",
    "fastify-formbody": "^2.0.0",
    "fetch": "^1.1.0",
    "googleapis": "^32.0.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.2",
    "html-pdf": "^2.2.0",
    "io-ts": "^1.1.5",
    "io-ts-reporters": "0.0.20",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "merge-graphql-schemas": "^1.5.3",
    "node-fetch": "^2.1.2",
    "node-uuid": "^1.4.8",
    "nodemailer": "^4.6.7",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "tslib": "^1.9.2",
    "twilio": "^3.18.0",
    "uuid": "^3.3.0"
  },
  "devDependencies": {
    "@streamrail/dsui": "^0.2.2",
    "@types/bcryptjs": "^2.4.1",
    "@types/bunyan": "^1.8.4",
    "@types/config": "0.0.34",
    "@types/csvtojson": "^1.1.5",
    "@types/ejs": "^2.6.0",
    "@types/express": "^4.0.39",
    "@types/express-session": "^1.15.10",
    "@types/faker": "^4.1.2",
    "@types/google-cloud__storage": "^1.1.8",
    "@types/graphql": "^0.13.1",
    "@types/html-pdf": "^2.1.2",
    "@types/jest": "^21.1.8",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lodash": "^4.14.110",
    "@types/node": "^9.3.0",
    "@types/node-fetch": "^2.1.1",
    "@types/node-uuid": "0.0.28",
    "@types/nodemailer": "^4.6.2",
    "@types/passport": "^0.4.5",
    "@types/passport-google-oauth": "^1.0.38",
    "@types/passport-local": "^1.0.33",
    "@types/passport-saml": "^0.15.6",
    "@types/supertest": "^2.0.4",
    "@types/twilio": "0.0.9",
    "@types/uuid": "^3.4.3",
    "chokidar": "^2.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.0",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "1.13.7",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-mockito": "^2.3.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2",
    "typescript": "^3.0.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
