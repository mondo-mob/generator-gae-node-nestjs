{
  "name": "<%= slugify(project) %>-server",
  "version": "1.0.0",
  "description": "<%= slugify(projectName) %> built with node + typescript + react",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start:dev": "CLOUDSDK_CORE_PROJECT=<%= slugify(project) %>-development npx server",
    "format": "prettier --write \"./src/**/*.ts\" && prettier --write \"./src/**/*.graphqls\"",
    "lint": "tslint --project .",
    "start": "npm run lint && node ./dist/main.js",
    "prestart:dev": "rm -rf dist/",
    "start:debug": "ts-node --inspect src/main.ts",
    "compile:prod": "rm -rf dist && tsc",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build": "rm -rf public && npm run lint && npm run --prefix ../client build && npm run compile:prod",
    "deploy:dev": "CLOUDSDK_CORE_PROJECT=<%= slugify(project) %>-dev npm run deploy && TAG_NAME=dev/`date +%Y%m%d_%H%M%S` && git tag -a $TAG_NAME -m 'dev deployed' && git push origin $TAG_NAME",
    "deploy:uat": "CLOUDSDK_CORE_PROJECT=<%= slugify(project) %>-uat npm run deploy && TAG_NAME=uat/`date +%Y%m%d_%H%M%S` && git tag -a $TAG_NAME -m 'uat deployed' && git push origin $TAG_NAME",
    "deploy:prod": "CLOUDSDK_CORE_PROJECT=<%= slugify(project) %>-prod npm run deploy && TAG_NAME=prod/`date +%Y%m%d_%H%M%S` && git tag -a $TAG_NAME -m 'prod deployed' && git push origin $TAG_NAME",
    "deploy": "npm run build && gcloud app deploy cron.yaml index.yaml app.yaml --quiet",
    "dsui": "dsui --port 4000",
    "kill": "bash ./scripts/kill.sh",
    "kill-force": "bash ./scripts/kill.sh -9"
  },
  "dependencies": {
    "@3wks/gae-node-nestjs": "1.2.0",
    "@google-cloud/connect-datastore": "2.0.1",
    "@google-cloud/datastore": "3.0.1",
    "@google-cloud/debug-agent": "3.0.1",
    "@google-cloud/logging-bunyan": "0.9.5",
    "@google-cloud/storage": "2.3.4",
    "@google-cloud/trace-agent": "3.5.0",
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/testing": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.12",
    "colors": "^1.3.0",
    "config": "3.0.1",
    "csurf": "^1.9.0",
    "csvtojson": "^2.0.7",
    "dataloader": "^1.4.0",
    "ejs": "^2.6.1",
    "email-addresses": "^3.0.1",
    "faker": "^4.1.0",
    "fastify-formbody": "^2.0.0",
    "fetch": "^1.1.0",
    "googleapis": "36.0.0",
    "graphql": "14.1.1",
    "graphql-tools": "4.0.3",
    "html-pdf": "^2.2.0",
    "io-ts": "^1.1.5",
    "io-ts-reporters": "0.0.21",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "merge-graphql-schemas": "^1.5.3",
    "node-fetch": "^2.1.2",
    "node-uuid": "^1.4.8",
    "nodemailer": "5.1.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "tslib": "^1.9.2",
    "twilio": "^3.18.0",
    "uuid": "^3.3.0"
  },
  "devDependencies": {
    "@streamrail/dsui": "^0.2.2",
    "@types/bcryptjs": "^2.4.1",
    "@types/bunyan": "^1.8.4",
    "@types/config": "0.0.34",
    "@types/csvtojson": "^1.1.5",
    "@types/ejs": "^2.6.0",
    "@types/express": "^4.0.39",
    "@types/express-session": "^1.15.10",
    "@types/faker": "^4.1.2",
    "@types/google-cloud__storage": "^1.1.8",
    "@types/graphql": "14.0.5",
    "@types/html-pdf": "^2.1.2",
    "@types/jest": "23.3.13",
    "@types/jsonwebtoken": "8.3.0",
    "@types/lodash": "^4.14.110",
    "@types/node": "10.12.18",
    "@types/node-fetch": "^2.1.1",
    "@types/node-uuid": "0.0.28",
    "@types/nodemailer": "^4.6.2",
    "@types/passport": "1.0.0",
    "@types/passport-google-oauth": "^1.0.38",
    "@types/passport-local": "^1.0.33",
    "@types/passport-saml": "^0.15.6",
    "@types/supertest": "^2.0.4",
    "@types/twilio": "0.0.10",
    "@types/uuid": "^3.4.3",
    "chokidar": "^2.0.4",
    "clean-webpack-plugin": "1.0.0",
    "concurrently": "4.1.0",
    "jest": "23.6.0",
    "nodemon": "^1.14.1",
    "prettier": "1.16.0",
    "supertest": "^3.0.0",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.3",
    "ts-mockito": "^2.3.0",
    "ts-node": "7.0.1",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "typescript": "^3.0.1",
    "webpack": "^4.2.0",
    "webpack-cli": "3.2.1",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
