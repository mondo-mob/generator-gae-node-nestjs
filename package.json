{
  "name": "@3wks/generator-gae-node-nestjs",
  "version": "0.5.1",
  "description": "A generator for building nestjs apps on app engine node",
  "repository": "github:3wks/generator-gae-node-nestjs",
  "author": {
    "name": "Glenn Molnar",
    "email": "glenn.molnar@growthops.com.au",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "node",
    "gae",
    "app engine",
    "nestjs",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "5.12.1",
    "eslint-config-prettier": "3.6.0",
    "eslint-config-xo": "0.26.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "^0.14.3",
    "jest": "24.7.1",
    "lint-staged": "8.1.0",
    "prettier": "1.16.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "2.4.2",
    "change-case": "3.1.0",
    "lodash": "4.17.11",
    "pluralize": "^7.0.0",
    "slugify": "1.3.4",
    "yeoman-generator": "3.2.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "npm audit --audit-level high",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "license": "MIT"
}
